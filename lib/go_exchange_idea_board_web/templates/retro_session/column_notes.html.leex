<%= for note <- @notes do %>
  <div id="<%= note.id %>">
    <%= note.content %>
    <%= link "Edit", to: Routes.live_path(@socket, NoteLive.Edit, note.id), class: "btn btn-primary btn-sm", phx_click: "edit-note", data_target: "#note-modal", data_toggle: "modal", "phx-value-retro-session-id": @retro_session_id, "phx-value-column-id": @column_id, "phx-value-column-title": @column_title, "phx-value-id": note.id, "phx-value-edit-mode": "true" %>
    <%= link "X", to: '#',
            phx_click: "delete-note",
            phx_value: note.id,
            data: [confirm: "Are you sure?"] %>
  </div>  
<% end %>